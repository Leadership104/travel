	<script>
		jQuery('.register_check_terms').click(function(e){
			if(jQuery('#checkboxChecked').is(':checked'))
			{
				jQuery('#invalidCheckBox').addClass('display-none');
				jQuery('#accepttermsandcondition').removeClass('text-danger');

			}
			else
			{
				jQuery('#invalidCheckBox').removeClass('display-none');
				jQuery('#accepttermsandcondition').addClass('text-danger');
				e.preventDefault();
				jQuery('#accepttermsandconditionmodal').modal('show');
			}
		})
		jQuery(document).ready(function(){
			jQuery('[data-toggle="tooltip"]').tooltip();
			jQuery('.input-icon-inline input').after('<i class="fa fa-eye-slash showhide_on_click"></i>');

			jQuery('.showhide_on_click').click(function(){
				if(jQuery(this).hasClass('activee'))
				{
					jQuery(this).parent().find('input').attr('type','password');
					jQuery(this).removeClass('activee').addClass('fa-eye-slash').removeClass('fa-eye');
				}
				else
				{
					jQuery(this).parent().find('input').attr('type','text');
					jQuery(this).addClass('activee').removeClass('fa-eye-slash').addClass('fa-eye');
				}
			})
		});
	</script>
